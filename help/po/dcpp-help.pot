msgid ""
"<b>%W</b> - Week of year as decimal number, with Monday as first day of week "
"(00 - 53)"
msgstr ""

#. type: Content of: <html><body><dl><dd><div>
#: help/settings_logs.html:204
msgid "<b>%x</b> - Date representation for current locale"
